@{
    var RoleId = "";
    if (Session["RoleId"] != null)
    {
        RoleId = Session["RoleId"].ToString();
    }

    var ActionName = this.Request.RequestContext.RouteData.Values["action"].ToString();
    var ContollerName = this.Request.RequestContext.RouteData.Values["controller"].ToString();
    if (ContollerName.ToLower() == "pricemgt".ToLower())
    {
        if (ActionName.ToLower() == "index".ToLower())
        {
            ViewBag.classpricemgt = "treeview menu-open";
        }

    }
    else
    {
        ViewBag.classpricemgt = "treeview ";
    }
    if (ContollerName.ToLower() == "Home".ToLower())
    {
        if (ActionName.ToLower() == "UserManagement".ToLower())
        {
            ViewBag.classUser = "treeview menu-open";
        }
        else if (ActionName.ToLower() == "VideoManagement".ToLower())
        {
            ViewBag.classVideo = "treeview menu-open";
        }
        else
        {
            ViewBag.classVideo = "treeview ";
            ViewBag.classUser = "treeview ";
        }
    }
    else
    {
        ViewBag.classUser = "treeview ";
    }




    if (ContollerName.ToLower() == "content".ToLower())
    {
        if (ActionName.ToLower() == "index".ToLower())
        {
            ViewBag.classcontent = "treeview active";
        }

    }
    else
    {
        ViewBag.classcontent = "treeview ";
    }
    if (ContollerName.ToLower() == "ClintMGT".ToLower())
    {

        ViewBag.classClint = "treeview active";


    }
    else
    {
        ViewBag.classcontent = "treeview ";
    }
    if (ContollerName.ToLower() == "DemoMGT".ToLower())
    {

        ViewBag.classDemo = "treeview active";


    }
    else
    {
        ViewBag.classDemo = "treeview ";
    }


}
