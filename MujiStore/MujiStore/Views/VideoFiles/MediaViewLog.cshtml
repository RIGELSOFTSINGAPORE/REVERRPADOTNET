@{
    Layout = null;
    var FielDtl = (MujiStore.Models.VideoFeedBack)ViewData["VFeedback"];

    var userDtl = ViewData["getdata"] as IEnumerable<MujiStore.Models.tblMediaViewLog>;

}
<style>
    .my-custom-scrollbar {
        position: relative;
        height: 500px;
        overflow: auto;
    }

    .table-wrapper-scroll-y {
        display: block;
    }

    table {
        font-size: 14px;
    }

    .footer {
        background-color: #7f0019;
        text-align: center;
        text-decoration-color: white;
        min-height: 25px;
        position: fixed;
        right: 0;
        left: 0;
        z-index: 1030;
        color: white;
        bottom: 0;
    }
</style>
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/Content/Site.css" rel="stylesheet" />

<link href="~/Content/style.css" rel="stylesheet" />

<div class="body-content" style="padding:10px;margin-top:-60px">
    <h2 style="color:#ec2213; background-color:#e7bebb;  margin-left:-4px; margin-bottom:15px;">@MujiStore.Resources.Resource.Video @MujiStore.Resources.Resource.ViewLogs</h2>

    <h2 style="color:black;font-weight:600; background-color:#e7bebb; margin-left:-4px;  font-size:14px; margin-bottom:15px; word-break: break-all; text-align: justify;">@FielDtl.VideoTitle</h2>
    <div class="table-wrapper-scroll-y my-custom-scrollbar">

        <table class="table " style="background-color:#ffffff; width:100%;">
            <thead>
                <tr class="thcolor">
                    <th style="width:10%">
                        @Html.DisplayName(MujiStore.Resources.Resource.ModFeedbackWriterDatetime)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayName(MujiStore.Resources.Resource.subnetID)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayName(MujiStore.Resources.Resource.ModtblStoreStoreName)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayName(MujiStore.Resources.Resource.ModtblUserUserName)
                    </th>
                    <th class="" style="width:10%">
                        @Html.DisplayName(MujiStore.Resources.Resource.Imagequalitysetting)
                    </th>
                    <th style="width:10%">
                        @Html.DisplayName(MujiStore.Resources.Resource.VieSearchClientIP)
                    </th>
                    <th style="width:20%">
                        @Html.DisplayName(MujiStore.Resources.Resource.browser)
                    </th>

                </tr>
            </thead>
            <tbody style="width:100%">
                @foreach (var item in userDtl)
                {
                <tr style="background-color:#EEE;">
                    <td class="">
                        @Html.DisplayFor(modelItem => item.STRCRTDT)
                        <br />
                        @Html.DisplayFor(modelItem => item.STRCRTDTTIME)
                    </td>
                    <td class="">
                        @Html.DisplayFor(modelItem => item.ClientSubnetID)
                    </td>
                    <td class="">
                        @Html.DisplayFor(modelItem => item.StoreName)
                    </td>
                    <td class="">
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td class="">
                        @Html.DisplayFor(modelItem => item.FormatName)
                    </td>
                    <td class="">
                        @Html.DisplayFor(modelItem => item.ClientIP)
                    </td>
                    <td class="">
                        @Html.DisplayFor(modelItem => item.UserAgent)
                    </td>
                </tr>
                }
            </tbody>

        </table>
    </div>
    <br />
    <div styel="margin-bottom:-50px; ">
             <button class="btn btn-primary" style="color: #fff; background-color: #7f0019;" onclick="self.close()"> @MujiStore.Resources.Resource.VieMediaViewLogBackToVideo</button>
    </div>
</div>
<div class="footer">

    <footer>  </footer>
</div>
