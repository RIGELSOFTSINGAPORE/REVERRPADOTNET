@model Vasthu_Models.VS_CONTENT_MST

@{
    ViewBag.Title = "Content Master - Create";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

    <h2>Content Master - Create</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group row">
            @Html.LabelFor(model => model.CONTENT_NAME, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-3">
                @Html.EditorFor(model => model.CONTENT_NAME, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "200", @autocomplete = "off" } })
                @Html.ValidationMessageFor(model => model.CONTENT_NAME, "", new { @class = "text-danger" })
            </div>
        </div>



        <div class="form-group row">
            @Html.LabelFor(model => model.DELETE_FLAG, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">

                @Html.EditorFor(model => model.DELETE_FLAG)
                @Html.ValidationMessageFor(model => model.DELETE_FLAG, "", new { @class = "text-danger" })

            </div>
        </div>



        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" style="background-color:#1bbd36; color:white;width: 84px;"/>
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index", null, htmlAttributes: new { @class = "edit" })
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")

}