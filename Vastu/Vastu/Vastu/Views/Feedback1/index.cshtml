
@{
    ViewBag.Title = "Feedback1";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/assets/css/testi.css" rel="stylesheet">
<section id="homevastu" class="homevastu">
    <div class="col-lg-6" data-aos="fade-right">
        <h2>Home Vastu</h2>
    </div>
</section>
<div class="modal fade" id="CreateFeedbackModal" tabindex="-1" role="dialog" aria-labelledby="CreateVideoModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header" style="background-color: #1bbd36">
                <h5 class="modal-title" id="CreateVideoModalLabel">Feedback</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body createVideocontent">
                <div>
                    @* <div class="col-md-6 form-group">*@
                    Name
                    <input type="text" name="vastutype" class="form-control" id="name" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                    <div class="validate"></div>
                    @*</div>*@
                </div>
                <div>
                    @*<div class="col-md-6 form-group">*@
                    Address<input type="text" class="form-control" name="areat" id="address" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                    <div class="validate"></div>
                    @*</div>*@
                </div>
                <div>
                    @*<div class="col-md-6 form-group">*@
                    City<input type="text" name="direction" class="form-control" id="city" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                    <div class="validate"></div>
                    @*</div>*@
                </div>
                <div>
                    @*<div class="col-md-6 form-group">*@
                    State<input type="text" name="direction" class="form-control" id="state" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                    <div class="validate"></div>
                    @*</div>*@
                </div>
                <div>
                    @* <div class="col-md-6 form-group">*@
                    Country<input type="text" name="direction" class="form-control" id="country" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                    <div class="validate"></div>
                    @*</div>*@
                </div>
                <div>
                    @* <div class="col-md-6 form-group">*@
                    Comments<input type="text" name="direction" class="form-control" id="comment" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                    <div class="validate"></div>
                    @*</div>*@
                </div>
                <div>
                    @* <div class="col-md-6 form-group">*@
                    Image<input type="file" name="direction" class="form-control" id="file" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                    <div class="validate"></div>
                    @*</div>*@
                </div>
                @*<div class="text-center">
                        <button type="submit">Submit Feedback</button>
                    </div>*@
            </div>

            @*</div>*@
            <div class="modal-footer">
                <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-success btnscreateavemenu" onclick="savefeedback()">Save</button>
            </div>
        </div>
    </div>
</div>

<main id="main">
    <!-- ======= Home vastu ======= -->
    <section id="homevastu" class="homevastu">
        <div id="feedback">
            <a href="#" type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal">
                <img <img src="data:image/svg+xml;base64,"
                          alt="Feedback" title="Feedback Button" height="70px" onclick="modelopen()" />
            </a>
        </div>
        <div class="container" data-aos="fade-up">
            <div class="section-title">
                <h2>FeedBack / Testimonials</h2>

                @*<div class="text-right">
                        <input type="image" src="~/assets/img/th (3).jpg" alt="Submit" width="128" height="68" onclick="myfunction()">
                    </div>*@



                <div class="container">
                    @{ int j = 0;}

                    @foreach (var interior in ViewBag.feedback)
                    {
                        <div class="testimonial-card">
                            <div class="text"> @interior.COMMENTS_1<i class="fas fa-quote-right quote"></i></div>
                            <h1 class="textname">- @interior.NAME</h1>


                            <div class="footer">
                                <div class="image">
                                    @* <img src="~/assets/img/download.jpg" style="width:100px">*@
                                    <img src="~/Photo/@interior.IMAGE_URL_1 " style="width:100px">
                                </div>



                            </div>
                        </div>
                        j = j + 1;
                    }
                    @*<div class="testimonial-card">
                            <div class="text">
                                Seeking vastu consultation would specifically be of help when you are planning to rent or buy a premise. It is especially recommended in case you are facing difficulties in life or there is disharmony at home or at work.<i class="fas fa-quote-right quote"></i>
                            </div>



                            <div class="footer">
                                <div class="image">

                                </div>
                                <h3 class='person'>Nithya</h3>


                            </div>
                        </div>*@
                    @*<div class="testimonial-card">
                            <div class="text">
                                This is because, it is believed that different planets govern different directions and have specific influences on residents.
                                By understanding the effects of directions, one can cordially balance diverse forces.<i class="fas fa-quote-right quote"></i>
                            </div>



                            <div class="footer">
                                <div class="image">

                                </div>
                                <h3 class='person'>Kitya</h3>


                            </div>
                        </div>*@

                </div>
            </div>
        </div>
    </section><!-- End Home Vastu Section -->
</main><!-- End #main -->
<script>
    function modelopen() {
        //debugger;
         @*$.ajax({
                url: '@Url.Action("CreateVideoListData", "BusinessVastu")',
                type: 'GET',
                success: function (result) {
                    debugger;
                    //$('.createVideocontent').html(result);
                    $('#CreateVideoModal').modal('show');
                     },
                error: function (error) {
                    console.log(error);
                }
            });*@
        $('#CreateFeedbackModal').modal('show');
    }
    function savefeedback() {
        //debugger;
        var name = $('#name').val();
        var address = $('#address').val();
        var comment = $('#comment').val();
        var city = $('#city').val();
        var state = $('#state').val();
        var country = $('#country').val();
        var file = $("#file").get(0).files;
        var fileData = new FormData();

        for (var i = 0; i < file.length; i++) {
            fileData.append("file", file[i]);
        }
        fileData.append('name', name);
        fileData.append('address', address);
        fileData.append('comment', comment);
        fileData.append('city', city);
        fileData.append('state', state);
        fileData.append('country', country);


        $.ajax({
            url: '@Url.Action("SaveFeedback", "Feedback")',
            type: 'POST',
            contentType: false,
            processData: false,
            //data: { 'name': name, 'address': address, 'comment': comment, 'city': city, 'state': state, 'country': country ,'file':file},
            data: fileData,
                        success: function (result) {
                            $('#CreateFeedbackModal').modal('hide');
                            //if (result.errorcode == 0) {
                                //alert('Feedback Added Successfully');

                            //}
                            //else {
                            //    alert('Video Updation Failed.');
                            //}
                        },
                        error: function (error) {
                            console.log(error);
                        }

        });
    }
</script>
