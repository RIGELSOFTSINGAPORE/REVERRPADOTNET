<script>
     function modelopen() {
        $('#CreateFeedbackModal').modal('hide');
        $('#name').val('');
        $('#lname').val('');
                    $('#address').val('') ;
                    $('#comment').val('');
                    $('#city').val('');
                    $('#state').val('');
                    $('#country').val('@Vastu.Resources.Resource.Japan');
        document.getElementById('file').value = "";
        document.getElementById("error").innerHTML = "";
        document.getElementById("com").innerHTML = "";
        document.getElementById("file1").innerHTML = "";
        $('#CreateFeedbackModal').modal('show');
    }
    function savefeedback() {
        var fileInput = document.getElementById('file');
        var filePath = fileInput.value;

        var allowedExtensions = /(\.jpg|\.jpeg|\.png)$/i;

        if ($('#name').val() == "") {
            document.getElementById("error").innerHTML = "@Vastu.Resources.Resource.EnterName";
        }
        else if ($('#comment').val() == "") {
            document.getElementById("com").innerHTML = "@Vastu.Resources.Resource.Entercomments";
        }
        else if (filePath.length> 0  && !allowedExtensions.exec(filePath)) {
            document.getElementById("file1").innerHTML = "Invalid File Type";
        }

        else {
            var name = $('#name').val();
            var lname = $('#lname').val();
            var address = $('#address').val();
            var comment = $('#comment').val();
            var city = $('#city').val();
            var state = $('#state').val();
            var country = $('#country').val();
            var file = $("#file").get(0).files;
            var fileData = new FormData();

            for (var i = 0; i < file.length; i++) {
                fileData.append("file", file[i]);
            }
            fileData.append('name', name);
            fileData.append('lname', lname);
            fileData.append('address', address);
            fileData.append('comment', comment);
            fileData.append('city', city);
            fileData.append('state', state);
            fileData.append('country', country);



            $.ajax({
                url: '@Url.Action("SaveFeedback", "Feedback")',
                type: 'POST',
                contentType: false,
                processData: false,
                //data: { 'name': name, 'address': address, 'comment': comment, 'city': city, 'state': state, 'country': country ,'file':file},
                data: fileData,
                success: function (result) {
                    $('#CreateFeedbackModal').modal('hide');
                    $('#name').val('');
                    $('#lname').val('');
                    $('#address').val('') ;
                    $('#comment').val('');
                    $('#city').val('');
                    $('#state').val('');
                    $('#country').val('@Vastu.Resources.Resource.Japan');
                    document.getElementById('file').value = "";

                    alert('@Vastu.Resources.Resource.FeedbackAddedSuccesfully');
                    //if (result.errorcode == 0) {
                    //alert('Feedback Added Successfully');

                    //}
                    //else {
                    //    alert('Video Updation Failed.');
                    //}
                },
                error: function (error) {
                    console.log(error);
                }

            });
        }
    }
</script>
