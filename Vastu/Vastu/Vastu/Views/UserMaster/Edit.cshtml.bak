@model Vasthu_Models.VS_USER_MST

@{
    ViewBag.Title = @Vastu.Resources.Resource.UserMasterEdit;
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>@Vastu.Resources.Resource.UserMasterEdit</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        <hr />
        <div class="row">
            <div class="col-sm">
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.USER_ID)
                @Html.HiddenFor(model => model.LOGIN_CONFIRM_PASSWORD)
                @Html.HiddenFor(model => model.LOGIN_CONFIRM_NEW_PASSWORD)
                @Html.HiddenFor(model => model.CREATED_USER_NAME)
                @Html.HiddenFor(model => model.C_USER_DATE)
                @Html.HiddenFor(model => model.UPDATED_USER_NAME)
                @Html.HiddenFor(model => model.U_USER_DATE)

                <div class="form-group row" style="margin-left:0px;">
                    @Html.Label(Vastu.Resources.Resource.Loginname, htmlAttributes: new { @class = "control-label col-md-3", @style = "padding-left: 0px;display: block;" })
                    <div class="col-md-6" style="margin-left:-8px;">
                        @Html.EditorFor(model => model.LOGIN_NAME, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "100", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.LOGIN_NAME, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.Password, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.LOGIN_PASSWORD, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "100", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.LOGIN_PASSWORD, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.FirstName, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.FIRST_NAME, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.FIRST_NAME, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.MiddleName, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.MIDDLE_NAME, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.MIDDLE_NAME, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.LastName, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.LAST_NAME, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.LAST_NAME, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.Address1, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.ADDRESS_LINE1, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", rows = "4", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.ADDRESS_LINE1, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.Address2, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.ADDRESS_LINE2, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", rows = "4", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.ADDRESS_LINE2, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.City, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.CITY, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.CITY, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.Country, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.COUNTRY, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.COUNTRY, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>
            <div class="col-sm">
                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.Email, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.EMAIL_ID, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.EMAIL_ID, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.ContactNo, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.EditorFor(model => model.CONTACT_NO, new { htmlAttributes = new { @class = "form-control", @style = "border-radius: 6px;", @maxlength = "400", @autocomplete = "off" } })
                        @Html.ValidationMessageFor(model => model.CONTACT_NO, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.UserType, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.DropDownListFor(model => model.USER_TYPE, (List<SelectListItem>)ViewBag.UserTypeID, htmlAttributes: new { @class = "form-control", @style = "border-radius: 6px;" })
                        @Html.ValidationMessageFor(model => model.USER_TYPE, "", new { @class = "text-danger" })

                    </div>
                </div>



                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.IsDelete, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;", @maxlength = "400", @autocomplete = "off" })
                    <div class="col-md-6">

                        @Html.EditorFor(model => model.DELETE_FLAG)
                        @Html.ValidationMessageFor(model => model.DELETE_FLAG, "", new { @class = "text-danger" })

                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.CreatedUser, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.DisplayFor(model => model.CREATED_USER_NAME, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.CREATED_USER_NAME, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.CreatedDate, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.DisplayFor(model => model.C_USER_DATE, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.C_USER_DATE, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.Updateduser, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.DisplayFor(model => model.UPDATED_USER_NAME, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.UPDATED_USER_NAME, "", new { @class = "text-danger" })
                    </div>
                </div>


                <div class="form-group row">
                    @Html.Label(Vastu.Resources.Resource.UpdatedDate, htmlAttributes: new { @class = "control-label col-md-3", @style = "display: block;" })
                    <div class="col-md-6">
                        @Html.DisplayFor(model => model.U_USER_DATE, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.U_USER_DATE, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="@Vastu.Resources.Resource.Save" class="btn btn-default btnCreate" style="background-color:#1bbd36; color:white;width: 84px;" />
                    </div>
                </div>

                <div style="margin-left:16px;">
                    @Html.ActionLink(Vastu.Resources.Resource.BacktoList, "Index", null, htmlAttributes: new { @class = "edit" })
                </div>
            </div>
        </div>
    </div>
}



@section Scripts {
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
}
