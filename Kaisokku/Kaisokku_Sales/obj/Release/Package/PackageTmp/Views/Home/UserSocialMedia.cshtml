@model IEnumerable<RPA_Common.Social>
@{
    ViewBag.Title = "UserSocialMedia";
}
<style>
    .imgtd {
        width: 100px;
        padding-left: 15px;
        padding-top: 10px;
    }
</style>
<h2>@RPA_Sales.Resources.Resource.SocialMedia</h2>
<table>
    <tr>
        @foreach (var result in Model)
        {
            <td class="imgtd">
                <span>
                    <a href="@result.Url">
                        <img src="~/images/@result.FileName" />
                    </a>
                </span>
            </td>
        }
    </tr>
</table>