


@using RPA_Common;
@model SocialMedia
@{ Layout = null; }

<form>
    <div class="form-group">
        <label for="Title">@RPA_Sales.Resources.Resource.Title</label>
        <input type="text" class="form-control" id="CreateSocialTitle" aria-describedby="emailHelp">
        <p id="title" style="color:red;"></p>
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
    </div>
    <div class="form-group">
        <label for="Description">@RPA_Sales.Resources.Resource.URL</label>
        <input type="text" class="form-control" id="CreateSocialUrl" aria-describedby="updatedby">
        <p id="title" style="color:red;"></p>
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
    </div>
    <div class="form-group">
        <label for="Description">@RPA_Sales.Resources.Resource.UploadIcon</label>
        <input type="file" class="form-control" id="CreateSocialFile" multiple aria-describedby="updatedby">
        <p id="upload" style="color:red;"></p>
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
    </div>
</form>
