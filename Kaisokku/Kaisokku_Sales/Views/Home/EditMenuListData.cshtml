@using Kaisokku_Common;
@model EditMenuDetails
@{ Layout = null; }

<form>
    @Html.Hidden("EditMenuId",Model.MenuId)
    <div class="form-group">
        <label for="MenuName">@Kaisokku_Sales.Resources.Resource.MenuName</label>
        <input type="text" class="form-control" id="EditMenuName" value="@Model.MenuName" aria-describedby="emailHelp" >
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@

    </div>
    <div class="form-group">
        <label for="updatedby">@Kaisokku_Sales.Resources.Resource.UpdatedBy</label>
        <input type="text" class="form-control" id="EdtiMenuupdatedby" value="@Model.updatedby" aria-describedby="updatedby">
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
    </div>
</form>