


@using Kaisokku_Common;
@model PPT
@{ Layout = null; }

<form>
    <div class="form-group">
        <label for="Title">@Kaisokku_Sales.Resources.Resource.Title</label>
        <input type="text" class="form-control" id="CreateVideoTitle" aria-describedby="emailHelp">
        <p id="title" style="color:red;"></p>
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
    </div>
    <div class="form-group">
        <label for="Description">@Kaisokku_Sales.Resources.Resource.Description</label>
        <input type="text" class="form-control" id="CreateVideoDesc" aria-describedby="updatedby">
        <p id="des" style="color:red;"></p>
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
    </div>
    <div class="form-group">
        <label for="Description">@Kaisokku_Sales.Resources.Resource.UploadPPT</label>
        <input type="file" class="form-control" id="CreateVideoFile" multiple aria-describedby="updatedby">
        @*<input  <p id="upload" style="color:red;"></p>type="file" class="form-control" id="CreateVideoFile" multiple aria-describedby="updatedby">*@
        @*<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>*@
    </div>
</form>